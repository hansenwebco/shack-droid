<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	android:id="@+id/RelativeLayoutMainMenu" 
>

	<ImageView
		android:id="@+id/ImageViewMainMenuBackground" 
		android:layout_width="200dip" 
		android:layout_height="200dip"
		android:layout_alignBottom="@+id/RelativeLayoutMainMenu"
		android:layout_alignParentBottom="true"
		android:layout_alignRight="@+id/RelativeLayoutMainMenu"
		android:layout_alignParentRight="true"
		android:layout_marginRight="30dip"
		android:layout_marginBottom="30dip"
		>
	</ImageView> 	
		

	<ListView
		android:id="@android:id/list" 
		android:layout_width="wrap_content" 
		android:layout_height="wrap_content" 
		android:textColor="#FFFFFF"
		android:dividerHeight="1px"
		android:divider="#333333"
		android:cacheColorHint="#00000000"
		android:layout_alignTop="@+id/RelativeLayoutMainMenu"
	/>
	

	
</RelativeLayout>