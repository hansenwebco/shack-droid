<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
android:id="@+id/widget50"
android:layout_width="fill_parent"
android:layout_height="fill_parent"
android:orientation="vertical"
xmlns:android="http://schemas.android.com/apk/res/android"
>
<TextView
android:id="@+id/TextViewLabelSearchFor"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Search For"
>
</TextView>
<EditText
android:id="@+id/EditTextSearch"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text=""
android:textSize="14sp"
>
</EditText>
<TextView
android:id="@+id/TextViewLabelByUser"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="By User"
>
</TextView>
<EditText
android:id="@+id/EditTextByUser"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text=""
android:textSize="14sp"
>
</EditText>
<TextView
android:id="@+id/TextViewLabelParentAuthor"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Parent Author"
>
</TextView>
<EditText
android:id="@+id/EditTextParentAuthor"
android:layout_width="fill_parent"
android:layout_height="wrap_content"
android:text=""
android:textSize="14sp"
>
</EditText>

<Button
android:id="@+id/ButtonSearch"
android:layout_width="wrap_content"
android:layout_height="wrap_content"
android:text="Search!"
>
</Button>
</LinearLayout>
